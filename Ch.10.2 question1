class WeatherForecast:
    def __init__(self):
        self.skies = ""  # Attribute variable to store the skies forecast
        self.high = 0    # Attribute variable to store the high temperature forecast
        self.low = 0     # Attribute variable to store the low temperature forecast

    def set_skies(self, forecast):
        self.skies = forecast
        # This method accepts an argument 'forecast' and assigns it to the 'skies' attribute variable.

    def set_high(self, temperature):
        self.high = temperature
        # This method accepts an argument 'temperature' and assigns it to the 'high' attribute variable.

    def set_low(self, temperature):
        self.low = temperature
        # This method accepts an argument 'temperature' and assigns it to the 'low' attribute variable.

    def get_skies(self):
        return self.skies
        # This method returns the value of the 'skies' attribute.

    def get_high(self):
        return self.high
        # This method returns the value of the 'high' attribute.

    def get_low(self):
        return self.low
        # This method returns the value of the 'low' attribute.
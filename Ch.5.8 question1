def max_of_three(a, b, c):
    """
    Returns the largest of three integers.

    Parameters:
    - a (int): First integer
    - b (int): Second integer
    - c (int): Third integer

    Returns:
    int: The largest of the three integers
    """
    return max(a, b, c)

# Example usage:
result = max_of_three(5, 10, 3)
print(result)